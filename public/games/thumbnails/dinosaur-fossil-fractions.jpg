<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="earthGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#d63031"/>
      <stop offset="50%" style="stop-color:#e17055"/>
      <stop offset="100%" style="stop-color:#fdcb6e"/>
    </linearGradient>
    <linearGradient id="fossilGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ddd"/>
      <stop offset="100%" style="stop-color:#b2bec3"/>
    </linearGradient>
  </defs>
  
  <!-- Desert background -->
  <rect width="300" height="200" fill="url(#earthGradient)"/>
  
  <!-- Sand dunes -->
  <path d="M0,120 Q75,100 150,120 T300,120 L300,200 L0,200 Z" fill="#fdcb6e" opacity="0.8"/>
  <path d="M0,140 Q100,130 200,140 T300,140 L300,200 L0,200 Z" fill="#f39c12" opacity="0.6"/>
  
  <!-- Excavation site -->
  <ellipse cx="150" cy="130" rx="80" ry="40" fill="#8b4513"/>
  <ellipse cx="150" cy="125" rx="75" ry="35" fill="#a0522d"/>
  
  <!-- T-Rex skeleton parts -->
  <g transform="translate(120,100)">
    <!-- Skull (1/4 showing) -->
    <path d="M0,0 L20,0 L25,10 L20,20 L0,20 Z" fill="url(#fossilGradient)" stroke="#636e72" stroke-width="1"/>
    <circle cx="5" cy="8" r="2" fill="#2d3436"/>
    <polygon points="20,5 30,8 20,12" fill="url(#fossilGradient)"/>
  </g>
  
  <g transform="translate(160,110)">
    <!-- Ribs (1/2 showing) -->
    <path d="M0,0 Q10,-5 20,0 Q25,5 20,10 Q10,15 0,10 Z" fill="url(#fossilGradient)" stroke="#636e72" stroke-width="1"/>
    <path d="M0,15 Q10,10 20,15 Q25,20 20,25 Q10,30 0,25 Z" fill="url(#fossilGradient)" stroke="#636e72" stroke-width="1"/>
  </g>
  
  <g transform="translate(190,120)">
    <!-- Leg bone (1/3 showing) -->
    <ellipse cx="0" cy="0" rx="15" ry="4" fill="url(#fossilGradient)" stroke="#636e72" stroke-width="1"/>
    <rect x="-3" y="0" width="6" height="20" fill="url(#fossilGradient)" stroke="#636e72" stroke-width="1"/>
  </g>
  
  <!-- Fraction labels -->
  <g transform="translate(110,80)">
    <circle cx="0" cy="0" r="12" fill="#fff" opacity="0.9"/>
    <text x="0" y="4" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="#e17055">¼</text>
  </g>
  
  <g transform="translate(170,90)">
    <circle cx="0" cy="0" r="12" fill="#fff" opacity="0.9"/>
    <text x="0" y="4" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="#00b894">½</text>
  </g>
  
  <g transform="translate(200,100)">
    <circle cx="0" cy="0" r="12" fill="#fff" opacity="0.9"/>
    <text x="0" y="4" text-anchor="middle" font-family="Arial" font-size="12" font-weight="bold" fill="#6c5ce7">⅓</text>
  </g>
  
  <!-- Paleontologist tools -->
  <g transform="translate(80,140)">
    <!-- Brush -->
    <rect x="0" y="0" width="15" height="3" fill="#8b4513"/>
    <rect x="15" y="-2" width="8" height="7" fill="#d63031"/>
    <!-- Magnifying glass -->
    <circle cx="30" cy="5" r="8" fill="#74b9ff" opacity="0.3" stroke="#2d3436" stroke-width="2"/>
    <line x1="36" y1="11" x2="45" y2="20" stroke="#2d3436" stroke-width="2"/>
  </g>
  
  <!-- Dinosaur silhouette in background -->
  <g transform="translate(240,60)" opacity="0.2">
    <path d="M0,40 Q-10,35 -15,25 Q-12,15 -5,10 Q5,8 15,12 Q25,20 30,30 L35,25 Q40,30 35,35 L30,40 Q25,45 15,45 Q5,45 0,40" fill="#2d3436"/>
  </g>
  
  <!-- Title -->
  <rect x="10" y="160" width="280" height="30" rx="15" fill="rgba(0,0,0,0.7)"/>
  <text x="150" y="180" text-anchor="middle" font-family="Arial" font-size="16" font-weight="bold" fill="#fff">Dinosaur Fossil Fractions</text>
</svg>